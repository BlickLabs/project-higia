{% extends "partials/base.njk" %}

{% block meta %}
{% endblock %}

{% block title %}
Team
{% endblock %}

{% block content %}
  <div class="team-container">
    <div class="team-our-team">
      <div class="team-title-container">
        <p class="higia-font-helveticaneue-bold team-title">Our team</p>
        <span class="team-line"></span>
      </div>
      <p class="higia-font-helveticaneue-normal team-user">
        <span class="higia-font-helveticaneue-bold">
          <span id="js-hover-name">Julián Rios</span></span> | <span id="js-hover-area">CEO</span>
      </p>
      <p class="higia-font-helveticaneue-thin team-descrip">
        <span id="js-hover-description"></span>
      </p>
      <p><a href="" id="js-hover-linkedin" target="_blank">
        <img src="img/team/linkedin_negro.svg" alt="Linkedin"></a></p>
    </div>
    <div class="team-photos">
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="0">
          <img src="img/team/team1.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">Julián Rios</p>
            <span class="higia-font-helveticaneue-normal">Chief Executive Officer</span>
          </div>
        </div>
      </div>
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="1">
          <img src="img/team/team2.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">Alejandro Casar</p>
            <span class="higia-font-helveticaneue-normal">Co-Artificial Intelligence Officer (Co-AIO)</span>
          </div>
        </div>
      </div>
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="2">
          <img src="img/team/team3.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">Raymundo González</p>
            <span class="higia-font-helveticaneue-normal">Co-Artificial Intelligence Officer (Co-AIO)</span>
          </div>
        </div>
      </div>
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="3">
          <img src="img/team/team4.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">Fernando López</p>
            <span class="higia-font-helveticaneue-normal">Director of Software Engineering</span>
          </div>
        </div>
      </div>
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="4">
          <img src="img/team/team5.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">Antonio Torres</p>
            <span class="higia-font-helveticaneue-normal">Chief Technology Officer (CTO)</span>
          </div>
        </div>
      </div>
      <div class="team-photo-container">
        <div class="team-photo-img" data-hover-id="5">
          <img src="img/team/team6.jpg">
          <div class="team-photo-mask">
            {# mascara al hacer hover #}
            <p class="higia-font-helveticaneue-bold">José Ángel Lavariega</p>
            <span class="higia-font-helveticaneue-normal">Chief Innovation Officer (CINO)</span>
          </div>
        </div>
      </div>
    </div>
  </div>
{% endblock %}
