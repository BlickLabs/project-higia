.gtkub-bg
  position fixed
  background-color rgba(0,0,0,.85)
  top 0
  right 0
  bottom 0
  left 0
  z-index 14
  display flex
  justify-content center
  align-items center

.video-hidden
  display none

.gtkub-container
  position relative
  width 75%

.gtkub-close
  position absolute
  left 101%
  bottom 100%
  cursor pointer

  img
    width 25px

.particles-container
  background-color #fff
  position relative

.absolute-container
  position absolute
  top 0
  left 0
  right 0

.higia-btn
  border 0
  padding 11.5px 28px
  font-size remify(13px)
  color #fff
  letter-spacing 1.3px
  cursor pointer
  transition .2s background-color ease-in-out

.uppercase
  text-transform uppercase

.btn-pink
  background-color #ee9bbe
  &:hover
    background-color #f89dcc

.btn-gray
  background-color #4c4c4c
  &:hover
    background-color #616363

.lines-container
  position fixed
  top 38%
  left 40px
  z-index 13
  height 100px
  display flex
  flex-direction column
  justify-content space-between
  transition .3s transform ease

  &.hidden
    transform translateX(-70px)

.line-container
  height 15px
  padding 7px 0
  display flex
  align-items center
  width 50px
  cursor pointer

  &:hover
    .line
      width 75%
    .line.active
      width 100%

.line
  width 50%
  height 1px
  background #979797
  transition .25s width ease

  &.active
    width 100%
    background-color #ea8193

//*************** section_one ***************//
.section_one
  position relative
  padding 1px 1px 1px 1px
  margin-bottom 66.5px

.section_one-mask
  position absolute
  top 0
  left 0
  bottom 0
  right 0
  pointer-events none

.section_one-mask-container
  max-width 1000px
  text-align left
  padding 0 44px
  margin 132px 22px 0

.section_one-mask img
  filter grayscale(1)
  height 377px

.logoPink
  filter grayscale(0) !important
  transition filter .8s ease

.section_one-content
  max-width 1136px
  padding-top 43px
  padding-left 554px
  padding-right 30px
  margin 208px auto 152px

.section_one-title
  rfont-size(54px,550px,29px)
  color #4b4b4b
  margin 0 0 40px 0
  line-height 1.03
  span
    color #5e5e5e

.lifePink
  animation lifePink .8s
  animation-fill-mode forwards
  animation-timing-function ease-in-out

.facts
  max-width 1136px
  padding 0 42px
  center-block()
  display flex
  align-items center

.facts-title
  rfont-size(30px, 550px, 21px)
  color #4c4c4c
  margin auto 30px auto 0

.facts-content
  display flex
  flex-wrap wrap

.fact
  display flex
  flex-direction row-reverse
  align-items center
  margin-left 15px
  transition .2s transform ease-in-out
  &:hover
    transform scale(1.1)

.fact-content
  width 150px

.fact-dscrptn
  margin auto 0 auto 0
  rfont-size(12.5px, 550px, 12px)
  color #4c4c4c
  letter-spacing .2px
  font-weight lighter

.fact-nmbrs
  rfont-size(19px, 500px, 17px)
  display block

.fact-icon
  width 70px
  text-align center

@keyframes lifePink
  from
    color #5e5e5e

  to
    color #ee9bbe

//*************** section_two ***************//
.section_two
  padding 1px 1px 67.5px 1px
  text-align center

.section_two-title_container
  center-block()
  display flex
  align-items center
  justify-content center
  margin 0 0 30px 0

.section_two-title
  rfont-size(40px, 500px, 21px)
  color #4b4b4b
  display inline-block
  margin 0 15px 0 0

.section_two-title_divider
  display inline-block
  background-color #ee9bbe
  width 3px
  height 47px

.section_two-subtitle
  rfont-size(33px, 500px, 20px)
  color #4b4b4b
  display inline-block
  margin 0 0 0 15px

.section_two-description
  rfont-size(19.5px, 500px, 16px)
  color #4b4b4b
  text-align center
  max-width 570px
  margin 0 auto 20px auto
  letter-spacing .3px

.section_two-features
  center-block()
  padding 0
  text-align center
  margin 0 0 20px 0

  .feature
    display inline-block
    color #4b4b4b
    rfont-size(18px, 500px, 11px)
    text-transform uppercase
    padding-right 13px
    padding-left 10px
    letter-spacing 2.3px

    &:nth-child(1), &:nth-child(2)
      border-right 2px solid #4b4b4b

  .feature-divider
    background-color #4b4b4b
    width 2px
    height 100%
    display inline-block

#section_two-mobile-button
  display none

.hidden-description
  overflow hidden
  height 0
  transition .75s height ease
  margin 0 0 35px 0

  &.active
    padding-top 50px
    height 500px

.hidden-description-content
  color #4b4b4b
  font-size remify(30px)
  max-width 651px
  margin 0 auto 40px auto
  letter-spacing .5px
  line-height 35px

.eva-button
  width 45px
  height 45px
  padding 0
  border 0
  border-radius 50%
  margin 0 0 75px 0
  background-color #fff
  background-image url('../img/eva/arrow-down.svg')
  background-repeat no-repeat
  background-position center center
  box-shadow 0px 1px 3px 0px rgba(74,74,74,0.2)
  cursor pointer
  transition .75s transform ease

  &.active
    transform rotate(180deg)
    box-shadow 0px -1px 3px 0px rgba(74,74,74,0.2)
    margin 0 0 25px 0

.eva-container
  text-align center
  width 393px
  center-block()
  position relative

.eva
  width 320px
  background-color #fefffe

.eva-feature
  display flex
  width 270px
  position absolute

  &.temp
    top 125px
    left 95%

    .stroke
      animation-delay .2s

    .bubble
      animation-delay 1.2s

    .eva-feature-name
      animation-delay 1.9s

  &.color
    flex-direction row-reverse
    top 240px
    right 95%

    .stroke-container
      display flex
      justify-content flex-end

    .stroke
      animation-delay 2.8s

    .bubble
      animation-delay 3.2s

    .eva-feature-name
      animation-delay 4.1s

  &.text
    bottom 85px
    left 90%

    .stroke
      animation-delay 5s

    .bubble
      animation-delay 5.6s

    .eva-feature-name
      animation-delay 6.2s

.eva-feature-graph
  display flex
  align-items center
  margin-right 15px
  margin-bottom 42px

  &.color
    flex-direction row-reverse
    margin-right 0
    margin-left 15px

.stroke-container
  width 80px
  height 1px

.stroke
  width 0
  height 1px
  background-color #e9e9e9

.stroke-anim
  animation eva-stroke .5s
  animation-fill-mode forwards
  animation-timing-function ease

.bubble-container
  width 17px
  height 17px
  display flex
  justify-content center
  align-items center

.bubble
  border-radius 50%
  background-color #f4f4f4
  display flex
  align-items center
  justify-content center
  width 17px
  height 17px
  transform scale(0)

.bubble-anim
  animation eva-bubble .6s
  animation-fill-mode forwards
  animation-timing-function ease

.subbubble
  border-radius 50%
  background-color #d8d8d8
  width 5px
  height 5px

.eva-feature-name
  opacity 0
  color #4b4b4b
  rfont-size(15px, 500px, 8px)
  letter-spacing 3.5px
  text-transform uppercase

.feature-anim
  animation eva-feature .8s
  animation-timing-function ease
  animation-fill-mode forwards

@keyframes eva-stroke
  0%
    width 0

  100%
    width 100%

@keyframes eva-bubble
  0%
    transform scale(0)

  66%
    transform scale(1.1)

  100%
    transform scale(1)

@keyframes eva-feature
  0%
    opacity 0

  100%
    opacity 1

//*************** section_three ***************//
.section_three
  background-image url('../img/backgrounds/section-3.png')
  height 41.5vw
  background-size cover
  display flex
  justify-content flex-end

.section_three-container
  width 38%
  display flex
  justify-content flex-start
  align-items center

.section_three-content
  width 230px

.section_three-title
  color #4b4b4b
  rfont-size(28px, 500px, 21px)
  letter-spacing .1px
  margin 0 0 10px 0

.section_three-description
  color #4b4b4b
  rfont-size(20px, 500px, 17px)
  letter-spacing .7px
  margin 0 0 30px 0

//*************** section_four ***************//
.section_four
  padding-top 70px
  background-color #fafafa
  padding-bottom 72px

.section_four-title
  rfont-size(37px, 500px, 21px)
  color #4b4b4b
  text-align center
  margin-top 47px

.section_four-slider-container
  max-width 491px
  center-block()
  display flex

.higia-slide
  text-align center
  outline 0
  padding 45px 0

  h3, p
    opacity 0

  img
    width 150px
    center-block()
    margin-bottom 21px
    transition .5s transform ease

.higia-slide.slick-current
  position relative
  .slider-container-description
    position absolute
    text-align center
    top 100%
    left -43%

  img
    transform scale(1.5)
    z-index 15
    border-radius 50%
    box-shadow 0px 3px 6px 0px rgba(0,0,0,0.6)
    border 0

  h3, p
    opacity 1

.slide-title
  color #4b4b4b
  rfont-size(19px, 500px, 16px)
  margin 0 10px

.slide-description
  color #4b4b4b
  rfont-size(17px, 500px, 13px)
  line-height 1.41
  width 265px

.arrow-slider
  background transparent
  border 0
  position relative

.arrow-right
  right -50px

.arrow-left
  left -50px

.arrow
  padding-bottom 220px
  width 10px

//*************** section_five ***************//
.section_five
  height 29.5vw
  background-color #fff
  display flex
  position relative

.circuit-mask
  background-image url('../img/backgrounds/circuit-mask.png')
  position absolute
  z-index 1
  background-size cover
  width 100%
  top 0
  right 0
  bottom 0
  left 0
  opacity 0

.circuit-anim
  animation circuit-opacity 1s ease forwards

.cupper-thing
  background-image url('../img/backgrounds/cupper-thing.png')
  position absolute
  background-size cover
  z-index 2
  top 0
  right 0
  bottom 0
  left 0

.section_five-container
  width 50%
  position absolute
  top 0
  right 0
  bottom 0
  left 0
  display flex
  justify-content flex-end
  align-items center
  z-index 3

.section_five-content
  display inline-block
  margin-right 12.5%

.section_five-title
  rfont-size(34px, 500px, 21px)
  color #4b4b4b
  margin 0 0 30px 0

.section_five
  .higia-btn
    padding 12px 15px

@keyframes circuit-opacity
  0%
    opacity 0
  100%
    opacity 1

//*************** section_six ***************//
.section_six
  padding 120px 0 100px 0
  background-color #f9faf9

.articles
  display flex
  flex-wrap wrap
  max-width 890px
  center-block()
  justify-content space-between

.article
  width 370px
  margin-bottom 84px

  .read_all-button
    display none

.article-author
  margin 0 0 30px 0
  &.expansion
    height 27px

  &.forbes
    height 26px

  &.elun
    height 26px

  &.bbc
    height 31px

.article-highlight
  margin 0 0 20px 0
  height 54px
  rfont-size(14px, 500px, 12px)
  color #5D5E5D
  line-height 1.27
  letter-spacing 0.5px
  font-weight lighter

.article-button
  padding 10px 18px
  border 0
  background-color #ee9bbe
  color #fff
  font-size remify(13px)
  letter-spacing 1.3px
  cursor pointer
  transition .2s background-color ease-in-out

.articles-read_all
  text-align center

.read_all-button
  padding 12px 35px
  background-color #4c4c4c
  color #fff
  font-size remify(13px)
  border 0
  letter-spacing 1.3px
  cursor pointer
  transition .2s background-color ease-in-out

//*************** section_seven ***************//
.section_seven
  padding 130px 0 130px 0

.section_seven-container
  max-width 640px
  center-block()

.section_seven-title
  margin 0 0 20px 0
  rfont-size(50px, 500px, 21px)
  color #4b4b4b
  letter-spacing .1px

.section_seven-subtitle
  margin 0 0 50px 0
  rfont-size(30px, 500px, 17px)
  color #4b4b4b
  letter-spacing .1px

.input-wrapper
  margin-bottom 50px
  position relative

.section_seven-field
  border 0
  border-bottom solid 1px #e1e1e1
  width 540px
  height 80px
  padding 35px 5px 5px 10px
  rfont-size(17px, 500px, 15px)
  color #4b4b4b
  letter-spacing .1px
  &::placeholder
    font-size remify(17px)
    letter-spacing .1px

.section_seven-field:focus ~ .label-placeholder, .section_seven-field:valid ~ .label-placeholder
  top 15%
  left 0
  rfont-size(19px, 500px, 17px)

.label-placeholder
  color #4d4d4d
  font-weight $bold
  rfont-size(21px, 500px, 17px)
  position absolute
  top 50%
  left .5rem
  transition .4s all ease

.newsletter-button
  padding 11px 22px
  background-color #ee9bbe
  color #fff
  font-size remify(13px)
  letter-spacing 1.3px
  border 0
  cursor pointer
  transition .2s background-color ease-in-out

